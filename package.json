{
  "name": "dfa.js",
  "version": "1.4.1",
  "description": "Javascript class library that models DFA(Deterministic Finite Automaton)",
  "main": "dfa.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "build": "gulp build",
    "test": "mocha --use_strict --compilers coffee:coffeescript --require coffeescript/register -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fxfan/dfa.js.git"
  },
  "author": {
    "name": "fxfan",
    "email": "fanban.xiaofan@gmail.com"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/fxfan/dfa.js/blob/master/LICENSE-MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/fxfan/dfa.js/issues"
  },
  "homepage": "https://github.com/fxfan/dfa.js#readme",
  "keywords": [
    "dfa",
    "javascript"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "target": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "coffee-script": "^1.12.7",
    "coffeescript": "^2.1.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-uglify": "^3.0.0",
    "mocha": "^4.1.0",
    "should": "^13.2.1"
  }
}
